@model abhinav_core.Models.logmodalClass



<form asp-controller="Home" asp-action="Login1" method="post">
  
  
    <h4>Login Employee</h4>
   

    <div class="form-group">
        <label>Email</label>
        <span style="color:red;">@TempData["invalid"]</span>
        <input required asp-for="Email" type="email"class="form-control" />
    </div>

    <div class="form-group">
        <label>Password</label>
        <span style="color:red;">@TempData["wrong"]</span>
        <input required asp-for="Pass" type="password" class="form-control" />
    </div>

    <button type="submit" class="btn btn-success">Login</button>
</form>